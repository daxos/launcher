Public Sub movimiento_menu()
 Dim f As Integer
  Dim g As Integer
  For f = 0 To 0 ' si fueran 10 objetos se cambia el 0 por el 10
    For g = -62 To 2
      f_menu.x = g ' si quieres cambiar de arriba hacia abajo cambiar x por y
      Wait (0.002)
    Next
  Next
End
