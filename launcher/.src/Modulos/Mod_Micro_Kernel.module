Public Sub Llenar_Combo_CC_Tema()
  cx = Mod_DB.conectar()
  rs = cx.Exec("select theme from tb_theme order by theme") 

Do While rs.Available 
  f_config.cmbtema.Add(rs!theme) 
  rs.MoveNext 
Loop 

End

