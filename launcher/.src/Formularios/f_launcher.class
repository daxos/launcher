Public Sub Form_Drop()
  
Dim a As String
Dim cadena As String
Dim estado As String

  a = Drag.Paste()
  a = RTrim(a)
  a = Replace$(a, "%20", " ")
  a = Replace$(a, "%C3%A1", "á")
  a = Replace$(a, "%C3%A9", "é")
  a = Replace$(a, "%C3%AD", "í")
  a = Replace$(a, "%C3%B3", "ó")
  a = Replace$(a, "%C3%BA", "ú")
  a = Replace$(a, "%C3%81", "Á")
  a = Replace$(a, "%C3%89", "É")
  a = Replace$(a, "%C3%8D", "Í")
  a = Replace$(a, "%C3%93", "Ó")
  a = Replace$(a, "%C3%9A", "Ú")
  a = Replace$(a, "%C3%B1", "ñ")
  a = Replace$(a, "%C3%91", "Ñ")
  a = Replace$(a, "file://", "")
  a = Replace$(a, "%5B", "[")
  a = Replace$(a, "%5D", "]")
  a = Replace$(a, " ", " ")
  
  Shell "dropbox status" To estado
  
  panel2.Show 
  IconViewCategoriasSelect.Hide
  principal.Hide
  Wait
  Shell "cp " & a & " ~/Dropbox/" 
  
End

